<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <!--@android:color/transparent-->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="ToolBarTextAppearance">
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="WebViewTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:statusBarColor">#c4c3c3</item>
    </style>
    <style name="Login_theme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="android:windowTranslucentStatus">true</item>
    </style>
    <style name="PhotoSelectTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:statusBarColor">#2b2b2b</item>
    </style>
</resources>
